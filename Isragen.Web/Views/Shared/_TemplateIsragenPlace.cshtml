@model int

<div class="form-group">
    <label class="control-label col-md-2" for="Places_'@Model'__Country">Country</label>
    <div class="col-md-10">
        <input class="form-control text-box single-line" id="Places_'@Model'__Country" name="Places['@Model'].Country" type="text" value="" />
        <span class="field-validation-valid text-danger" data-valmsg-for="Places['@Model'].Country" data-valmsg-replace="true"></span>
    </div>
</div>

<div class="form-group">
    <label class="control-label col-md-2" for="Places_'@Model'__City">City</label>
    <div class="col-md-10">
        <input class="form-control text-box single-line" id="Places_'@Model'__City" name="Places['@Model'].City" type="text" value="" />
        <span class="field-validation-valid text-danger" data-valmsg-for="Places['@Model'].City" data-valmsg-replace="true"></span>
    </div>
</div>
